<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.wangshuai.androidui"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".material.MaterialDesignActivity">
        </activity>
        <activity android:name=".material.PaletteActivity">
        </activity>
        <activity android:name=".material.TabLayoutActivity">
        </activity>
        <activity
            android:name=".material.TranslucentActivity"
            android:theme="@style/NoTitleTheme">
        </activity>
        <activity
            android:name=".material.DrawLayoutActivity"
            android:label="@string/title_activity_draw_layout"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".material.CardViewActivity">
        </activity>
        <activity android:name=".material.SnackBarActivity">
        </activity>
        <activity android:name=".material.FloatingActionButtonActivity">
        </activity>
        <activity
            android:name=".material.CoordinatorLayoutActivity"
            android:theme="@style/NoActionBarTheme">
        </activity>
        <activity
            android:name=".material.AppBarLayoutActivity"
            android:theme="@style/NoActionBarTheme">
        </activity>
        <activity
            android:name=".material.CollapsingToolbarLayoutActivity"
            android:theme="@style/NoActionBarTheme">
        </activity>
        <activity android:name=".material.BottomNavigationViewActivity">
        </activity>
        <activity android:name=".material.TextInputLayoutActivity">
        </activity>
        <activity android:name=".material.BottomSheetActivity">
        </activity>
        <activity android:name=".material.CustomBehaviorActivity">
        </activity>
    </application>

</manifest>